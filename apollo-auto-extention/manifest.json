{
  "manifest_version": 3,
  "name": "Apollo Auto Cookie Extractor",
  "description": "提取 MayoHR 系統的 cookies",
  "version": "0.1.0",
  "minimum_chrome_version": "110",
  "action": {
    "default_title": "Apollo Auto Cookie Extractor",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": ["cookies"],
  "host_permissions": ["https://*.mayohr.com/*"]
}
